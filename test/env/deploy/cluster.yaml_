---
apiVersion: contrail.juniper.net/v1alpha1
kind: Manager
metadata:
  name: cluster1
  namespace: contrail
spec:
  commonConfiguration:
    hostNetwork: true
    replicas: 1
  keystoneSecretInstance: cluster1-admin-password
  services:
    cassandras:
      - metadata:
          labels:
            contrail_cluster: cluster1
          name: cassandra1
        spec:
          commonConfiguration:
            create: true
            nodeSelector:
              node-role.kubernetes.io/master: ""
          serviceConfiguration:
            containers:
              - name: cassandra
                image: localhost:5000/cassandra:3.11.4
              - name: init
                image: localhost:5000/busybox:1.31
              - name: init2
                image: localhost:5000/cassandra:3.11.4
    config:
      metadata:
        labels:
          contrail_cluster: cluster1
        name: config1
      spec:
        commonConfiguration:
          create: true
          nodeSelector:
            node-role.kubernetes.io/master: ""
        serviceConfiguration:
          cassandraInstance: cassandra1
          keystoneInstance: keystone
          containers:
            - name: analyticsapi
              image: localhost:5000/contrail-analytics-api:2005.42
            - name: api
              image: localhost:5000/contrail-controller-config-api:2005.42
            - name: collector
              image: localhost:5000/contrail-analytics-collector:2005.42
            - name: devicemanager
              image: localhost:5000/contrail-controller-config-devicemgr:2005.42
            - name: dnsmasq
              image: localhost:5000/contrail-controller-config-dnsmasq:2005.42
            - name: init
              image: localhost:5000/python:3.8.2-alpine
            - name: init2
              image: localhost:5000/busybox:1.31
            - name: nodeinit
              image: localhost:5000/contrail-node-init:2005.42
            - name: schematransformer
              image: localhost:5000/contrail-controller-config-schema:2005.42
            - name: servicemonitor
              image: localhost:5000/contrail-controller-config-svcmonitor:2005.42
            - name: queryengine
              image: localhost:5000/contrail-analytics-query-engine:2005.42
            - name: redis
              image: localhost:5000/redis:4.0.2
            - name: statusmonitor
              image: localhost:5000/contrail-statusmonitor:R2005.2872176
          logLevel: SYS_DEBUG
          zookeeperInstance: zookeeper1
          authMode: keystone
    controls:
      - metadata:
          labels:
            contrail_cluster: cluster1
            control_role: master
          name: control1
        spec:
          commonConfiguration:
            create: true
            nodeSelector:
              node-role.kubernetes.io/master: ""
          serviceConfiguration:
            cassandraInstance: cassandra1
            containers:
              - name: control
                image: localhost:5000/contrail-controller-control-control:2005.42
              - name: dns
                image: localhost:5000/contrail-controller-control-dns:2005.42
              - name: init
                image: localhost:5000/python:3.8.2-alpine
              - name: named
                image: localhost:5000/contrail-controller-control-named:2005.42
              - name: nodemanager
                image: localhost:5000/contrail-nodemgr:2005.42
              - name: nodeinit
                image: localhost:5000/contrail-node-init:2005.42
              - name: statusmonitor
                image: localhost:5000/contrail-statusmonitor:R2005.2872176
            zookeeperInstance: zookeeper1
    provisionManager:
      metadata:
        labels:
          contrail_cluster: cluster1
        name: provmanager1
      spec:
        commonConfiguration:
          create: true
          nodeSelector:
            node-role.kubernetes.io/master: ""
          replicas: 1
        serviceConfiguration:
          keystoneInstance: keystone
          globalVrouterConfiguration:
            ecmpHashingIncludeFields:
              destinationIp: true
              destinationPort: true
              hashingConfigured: true
              ipProtocol: true
              sourceIp: true
              sourcePort: true
            encapPriority: VXLAN,MPLSoGRE,MPLSoUDP
            vxlanNetworkIdentifierMode: automatic
          containers:
            - name: init
              image: localhost:5000/python:3.8.2-alpine
            - name: provisioner
              image: localhost:5000/contrail-operator-provisioner:R2005.2872176
    rabbitmq:
      metadata:
        labels:
          contrail_cluster: cluster1
        name: rabbitmq1
      spec:
        commonConfiguration:
          create: true
          nodeSelector:
            node-role.kubernetes.io/master: ""
        serviceConfiguration:
          containers:
            - name: init
              image: localhost:5000/python:3.8.2-alpine
            - name: rabbitmq
              image: localhost:5000/rabbitmq:3.7
    webui:
      metadata:
        labels:
          contrail_cluster: cluster1
        name: webui1
      spec:
        commonConfiguration:
          create: true
          nodeSelector:
            node-role.kubernetes.io/master: ""
        serviceConfiguration:
          cassandraInstance: cassandra1
          keystoneInstance: keystone
          containers:
            - name: init
              image: localhost:5000/python:3.8.2-alpine
            - name: nodeinit
              image: localhost:5000/contrail-node-init:2005.42
            - name: redis
              image: localhost:5000/redis:4.0.2
            - name: webuijob
              image: localhost:5000/contrail-controller-webui-job:2005.42
            - name: webuiweb
              image: localhost:5000/contrail-controller-webui-web:2005.42
    zookeepers:
      - metadata:
          labels:
            contrail_cluster: cluster1
          name: zookeeper1
        spec:
          commonConfiguration:
            create: true
            nodeSelector:
              node-role.kubernetes.io/master: ""
          serviceConfiguration:
            containers:
              - name: init
                image: localhost:5000/python:3.8.2-alpine
              - name: zookeeper
                image: localhost:5000/zookeeper:3.5.5
    command:
      metadata:
        name: command
      spec:
        commonConfiguration:
          activate: true
          create: true
          hostNetwork: true
          tolerations:
            - effect: NoSchedule
              operator: Exists
            - effect: NoExecute
              operator: Exists
          nodeSelector:
            node-role.kubernetes.io/master: ""
        serviceConfiguration:
          configAPIURL: https://kind-control-plane:8082
          telemetryURL: https://kind-control-plane:8081
          postgresInstance: postgres
          swiftInstance: swift
          swiftProxyPort: 5080
          keystonePort: 5555
          keystoneInstance: keystone
          containers:
            - name: api
              image: localhost:5000/contrail-command:2005.42
            - name: init
              image: localhost:5000/contrail-command:2005.42
            - name: wait-for-ready-conf
              image: localhost:5000/busybox:1.31
    keystone:
      metadata:
        name: keystone
      spec:
        commonConfiguration:
          activate: true
          create: true
          hostNetwork: true
          tolerations:
            - effect: NoSchedule
              operator: Exists
            - effect: NoExecute
              operator: Exists
          nodeSelector:
            node-role.kubernetes.io/master: ""
        serviceConfiguration:
          memcachedInstance: "memcached"
          postgresInstance: postgres
          listenPort: 5555
          containers:
            - name: wait-for-ready-conf
              image: localhost:5000/busybox:1.31
            - name: keystoneDbInit
              image: localhost:5000/postgresql-client:1.0
            - name: keystoneInit
              image: localhost:5000/centos-binary-keystone:train-2005
            - name: keystone
              image: localhost:5000/centos-binary-keystone:train-2005
            - name: keystoneSsh
              image: localhost:5000/centos-binary-keystone-ssh:train-2005
            - name: keystoneFernet
              image: localhost:5000/centos-binary-keystone-fernet:train-2005
    postgres:
      metadata:
        name: postgres
      spec:
        containers:
          - name: postgres
            image: localhost:5000/postgres:12.2
          - name: wait-for-ready-conf
            image: localhost:5000/busybox:1.31
    memcached:
      metadata:
        name: memcached
        namespace: contrail
      spec:
        serviceConfiguration:
          containers:
            - name: memcached
              image: "localhost:5000/centos-binary-memcached:train-2005"
    swift:
      metadata:
        name: swift
      spec:
        serviceConfiguration:
          credentialsSecretName: "swift-credentials-secret"
          containers:
            - name: ring-reconciler
              image: localhost:5000/centos-source-swift-base:train-2005
          swiftProxyConfiguration:
            memcachedInstance: "memcached"
            keystoneInstance: "keystone"
            listenPort: 5080
            containers:
              - name: init
                image: localhost:5000/centos-binary-kolla-toolbox:train-2005
              - name: wait-for-ready-conf
                image: localhost:5000/busybox:1.31
              - name: api
                image: localhost:5000/centos-binary-swift-proxy-server:train-2005
          swiftStorageConfiguration:
            accountBindPort: 6001
            containerBindPort: 6002
            objectBindPort: 6000
            device: d1
            containers:
              - name: swiftObjectExpirer
                image: localhost:5000/centos-binary-swift-object-expirer:train-2005
              - name: swiftObjectUpdater
                image: localhost:5000/centos-binary-swift-object:train-2005
              - name: swiftObjectReplicator
                image: localhost:5000/centos-binary-swift-object:train-2005
              - name: swiftObjectAuditor
                image: localhost:5000/centos-binary-swift-object:train-2005
              - name: swiftObjectServer
                image: localhost:5000/centos-binary-swift-object:train-2005
              - name: swiftContainerUpdater
                image: localhost:5000/centos-binary-swift-container:train-2005
              - name: swiftContainerReplicator
                image: localhost:5000/centos-binary-swift-container:train-2005
              - name: swiftContainerAuditor
                image: localhost:5000/centos-binary-swift-container:train-2005
              - name: swiftContainerServer
                image: localhost:5000/centos-binary-swift-container:train-2005
              - name: swiftAccountReaper
                image: localhost:5000/centos-binary-swift-account:train-2005
              - name: swiftAccountReplicator
                image: localhost:5000/centos-binary-swift-account:train-2005
              - name: swiftAccountAuditor
                image: localhost:5000/centos-binary-swift-account:train-2005
              - name: swiftAccountServer
                image: localhost:5000/centos-binary-swift-account:train-2005
